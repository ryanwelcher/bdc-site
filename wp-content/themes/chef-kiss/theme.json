{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"version": 2,
	"customTemplates": [],
	"title": "Chef Kiss Theme",
	"settings": {
		"appearanceTools": true,
		"color": {
			"customDuotone": false,
			"customGradient": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#fff",
					"name": "Text",
					"slug": "text"
				},
				{
					"color": "#E76F51",
					"name": "Highlight",
					"slug": "highlight"
				},
				{
					"color": "#2D2B55",
					"name": "Background",
					"slug": "background"
				}
			]
		},
		"layout": {
			"contentSize": "60rem",
			"wideSize": "64rem"
		},
		"typography": {
			"dropCap": true,
			"fluid": true,
			"customFontSize": false,
			"fontSizes": [
				{
					"fluid": {
						"min": "0.8rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.3rem",
						"max": "1.875rem"
					},
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2.25rem",
					"slug": "x-large"
				}
			],
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Jost",
							"fontStyle": "normal",
							"fontWeight": "normal",
							"src": [
								"file:./assets/fonts/Jost/Jost-VariableFont_wght"
							]
						}
					],
					"fontFamily": "Jost",
					"name": "Jost",
					"slug": "jost"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Prata",
							"fontStyle": "normal",
							"fontWeight": "bold",
							"src": [
								"file:./assets/fonts/Prata/Prata-Regular.ttf"
							]
						}
					],
					"fontFamily": "Prata",
					"name": "Prata",
					"slug": "prata"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Space Mono",
							"fontStyle": "regular",
							"fontWeight": "regular",
							"src": [
								"file:./assets/fonts/Space_Mono/SpaceMono-Regular.ttf"
							]
						},
						{
							"fontFamily": "Space Mono",
							"fontStyle": "bold",
							"fontWeight": "bold",
							"src": [
								"file:./assets/fonts/Space_Mono/SpaceMono-Bold.ttf"
							]
						},
						{
							"fontFamily": "Space Mono",
							"fontStyle": "italic",
							"fontWeight": "bold",
							"src": [
								"file:./assets/fonts/Space_Mono/SpaceMono-BoldItalic.ttf"
							]
						},
						{
							"fontFamily": "Space Mono",
							"fontStyle": "italic",
							"fontWeight": "regular",
							"src": [
								"file:./assets/fonts/Space_Mono/SpaceMono-Italic.ttf"
							]
						}
					],
					"fontFamily": "Space Mono",
					"name": "Space Mono",
					"slug": "space-mono"
				},
				{
					"fontFamily": "Montserrat, sans-serif",
					"name": "Montserrat",
					"slug": "montserrat"
				}
			]
		},
		"useRootPaddingAwareAlignments": true,
		"blocks": {
			"chef-kiss/time-remaining": {
				"border": {
					"radius": true
				}
			}
		},
		"custom": {
			"chef-kiss-level": {
				"icon": "'üë®‚Äçüç≥'"
			}
		}
	},
	"styles": {
		"blocks": {
			"chef-kiss/time-remaining": {
				"color": {
					"text": "var( --wp--preset--color--highlight )",
					"background": "var( --wp--preset--color--card )"
				},
				"spacing": {
					"padding": {
						"top": "2rem",
						"right": "2rem",
						"bottom": "2rem",
						"left": "2rem"
					}
				},
				"typography": {
					"fontSize": "var( --wp--preset--font-size--x-large)"
				}
			},
			"chef-kiss/level": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--small )"
				}
			},
			"chef-kiss/cooking-time": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--small )"
				}
			},
			"core/post-excerpt": {
				"color": {
					"text": "var( --wp--preset--color--card )"
				}
			}
		},
		"color": {
			"background": "var( --wp--preset--color--background)",
			"text": "var( --wp--preset--color--text )"
		},
		"typography": {
			"fontFamily": "var( --wp--preset--font-family--jost )",
			"fontSize": "var( --wp--preset--font-size--large )"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var( --wp--preset--color--background)",
					"text": "var( --wp--preset--color--background)"
				}
			},
			"heading": {
				"color": {
					"text": "var( --wp--preset--color--highlight )"
				},
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--jost )",
					"fontSize": "var( --wp--preset--font-size--x-large )"
				}
			},
			"link": {
				"color": {
					"text": "var( --wp--preset--color--highlight )"
				}
			}
		},
		"spacing": {
			"blockGap": "2rem",
			"margin": {
				"bottom": "1rem",
				"left": "5rem",
				"right": "5rem",
				"top": "1rem"
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "sidebar",
			"name": "sidebar",
			"title": "Sidebar"
		}
	]
}
